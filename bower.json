{
  "name": "gps-node",
  "description": "A gps web app for FIT files",
  "main": "index.js",
  "authors": [
    "Ben Melville"
  ],
  "license": "BSD-2-Clause",
  "homepage": "https://github.com/jaminmelville/gps-node",
  "moduleType": [
    "node"
  ],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "react": "~0.14.5",
    "jquery": "~2.1.4",
    "highcharts": "~4.2.1",
    "foundation-sites": "~6.1.1",
    "motion-ui": "~1.2.2",
    "three.js": "~0.76.0",
    "requirejs": "~2.2.0",
    "perfect-scrollbar": "~0.6.11",
    "hammerjs": "~2.0.8"
  },
  "install": {
    "path": "public/js/vendor",
    "ignore": [
      "what-input"
    ],
    "sources": {
      "react": [
        "bower_components/react/react.js",
        "bower_components/react/react-dom.js"
      ],
      "foundation-sites": "bower_components/foundation-sites/dist/foundation.min.js",
      "jquery": "bower_components/jquery/dist/jquery.min.js",
      "highcharts": "bower_components/highcharts/highcharts.js"
    }
  }
}
